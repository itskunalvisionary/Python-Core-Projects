   "source": [
    "songs = [\"Shape of You\", \"Stay\", \"Blinding Lights\", \"Starboy\"]\n",
    "\n",
    "def search_song(query):\n",
    "    query = query.lower()\n",
    "    result = [song for song in songs if query in song.lower()]\n",
    "    return result\n",
    "\n",
    "print(search_song(\"YOU\"))"
   ]
